<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record id="view_partner_form" model="ir.ui.view">
			<field name="name">Partner newsletter prints</field>
			<field name="model">res.partner</field>
			<field name="inherit_id" ref="base.view_partner_form" />
			<field name="arch" type="xml">

				<notebook position="inside">
					<page name="newsletters" string="Newsletters" attrs="{'invisible': [('is_company', '=', False)]}">
						<field name="newsletter_prints" context="{'default_partner': active_id}">
							<tree editable="top">
								<field name="partner" invisible="1" />
								<field name="amount" />
								<field name="contact" />
							</tree>
						</field>
					</page>
				</notebook>

			</field>
		</record> 

	</data>
</openerp>